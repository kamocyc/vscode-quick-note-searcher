{
	"name": "quick-note-searcher",
	"displayName": "quick-note-searcher",
	"description": "Quick note (with tags) searcher for VS Code",
	"version": "0.0.1",
	"publisher": "kamocyc",
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:quick-note-searcher.show-search-box"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "quick-note-searcher.show-search-box",
				"title": "Quick note search"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "tslint -p ./",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/debounce": "^1.2.0",
		"@types/node": "^10.14.17",
		"@types/vscode": "^1.34.0",
		"tslint": "^5.20.0",
		"typescript": "^3.7.2"
	},
	"dependencies": {
		"debounce": "^1.2.0"
	}
}
